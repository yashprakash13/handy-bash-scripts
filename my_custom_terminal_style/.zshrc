export ZSH="/Users/username/.oh-my-zsh"
ZSH_THEME="dstufft"

# custom aliases
alias de="cd ~/Desktop";
alias mlap="cd ~/Desktop/folder1";
alias pydl="cd ~/Desktop/folder2";
alias gs="git status";
alias gb="git branch";
alias ga="git add .";
alias ..='cd ..'
alias home='cd'
alias ...='cd ../..'
alias cl="clear"

ZSH_DISABLE_COMPFIX="true"
source $ZSH/oh-my-zsh.sh
